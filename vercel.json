{
    "version": 2,
    "builds": [
      {
        "src": "public", 
        "use": "@vercel/static"
      },
      {
        
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "main.py"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }
  